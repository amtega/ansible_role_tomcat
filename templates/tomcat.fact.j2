---
{{ ansible_managed | comment }}

tomcat_user: "{{ tomcat_user }}"
tomcat_group: "{{ tomcat_group }}"
tomcat_base_path: "{{ tomcat_base_path }}"
tomcat_version: "{{ tomcat_version }}"
tomcat_version_major: "{{ tomcat_version_major }}"
tomcat_version_minor: "{{ tomcat_version_major }}"
tomcat_version_patch: "{{ tomcat_version_major }}"
tomcat_download_mirror: "{{ tomcat_download_mirror }}"

{% if tomcat_download_url is defined %}
tomcat_download_url: "{{ tomcat_download_url }}"
{% endif %}

tomcat_download_path: "{{ tomcat_download_path }}"
tomcat_home: "{{ tomcat_home }}"
